#c
import calendar
cal = calendar.TextCalendar()
n = int(input("Please enter month of birthday as a number (January = 1,etc): "))

def birthdaymonth(n):
    cal.prmonth(2012,n)

birthdaymonth(n)

#d
import calendar
d = calendar.LocaleTextCalendar(6, "Chinese")
d.pryear(2012)

